<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-9-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-9-0 http://www.eiffel.com/developers/xml/configuration-1-9-0.xsd" name="eiffelstore2-tests" uuid="671C9530-CB14-493E-8F77-39A919C5723A" library_target="eiffelstore2-tests">
	<description>Unit-tests for the EiffelStore2 library.</description>
	<target name="eiffelstore2-tests">
		<root class="ANY" feature="default_create"/>
		<file_rule>
			<exclude>/EIFGENs$</exclude>
			<exclude>/.svn$</exclude>
		</file_rule>
		<file_rule>
			<exclude>^/test_classes$</exclude>
		</file_rule>
		<option trace="false" cat_call_detection="false" is_attached_by_default="true" void_safety="all" syntax="provisional" namespace="EiffelSoftware.Library">
			<assertions precondition="true" postcondition="true" check="true" invariant="true" loop="true" supplier_precondition="true"/>
		</option>
		<setting name="dotnet_naming_convention" value="true"/>
		<setting name="concurrency" value="none"/>
		<library name="base" location="$ISE_LIBRARY\library\base\base-safe.ecf"/>
		<library name="eiffelstore2" location="..\..\..\libraries\ethz\src\abel\eiffelstore2.ecf" readonly="false">
			<option full_class_checking="true" void_safety="all">
			</option>
		</library>
		<library name="mysql" location="..\..\..\libraries\ethz\src\mysqli\mysqli.ecf"/>
		<library name="testing" location="$ISE_LIBRARY\library\testing\testing-safe.ecf"/>
		<cluster name="test_classes" location=".\test_classes\" recursive="true"/>
		<library name="sqlite" location="$ISE_LIBRARY\unstable\library\persistency\database\sqlite3\sqlite-safe.ecf"/>
		<tests name="tests" location=".\">
			<option void_safety="all" syntax="provisional" namespace="Tests">
			</option>
			<tests name="schema_evolution_version_1" location="..\..\..\src\tests\eweasel\tests\schema_evolution_version_1\"/>
			<tests name="schema_evolution_version_2" location="..\..\..\src\tests\eweasel\tests\schema_evolution_version_2\"/>
		</tests>
	</target>
</system>
