<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-9-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-9-0 http://www.eiffel.com/developers/xml/configuration-1-9-0.xsd" name="serialization" uuid="1EF56491-67FF-4209-91BB-B85927BDCB05">
	<target name="serialization">
		<root class="TEST_PS_SERIALIZER" feature="make"/>
