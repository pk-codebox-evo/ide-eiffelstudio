	date: "$Date$"
	revision: "$Revision$"

deferred class
	TEXTUAL_BON_ELEMENT

inherit
	TEXTUAL_BON_OUTPUT_STRATEGY

feature -- Processing
	process_to_informal_textual_bon
			-- Process this element into informal textual BON.
		deferred
		end

	process_to_formal_textual_bon
			-- Process this element into formal textual BON.
		deferred
		end

