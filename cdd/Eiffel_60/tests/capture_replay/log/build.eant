<?xml version="1.0"?>

<project name="argument" default="help" >

	<description>
		system: "Gobo Argument Parser Tester"
		copyright: "Copyright (c) 2006, Bernd Schoeller and others"
		license: "Eiffel Forum License v2 (see forum.txt)"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/misc/test.eant">
			<redefine target="init_system"/>
			<redefine target="other_clean"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xlog"/>
		<set name="library" value="log"/>
		<set name="system_dir" value="."/>
	</target>

	<target name="other_clean">
		<!-- delete all generated log's (excluding check files) -->
		<delete>
			<fileset include="*.log" />
		</delete>
		<delete directory="EIFGENs" />
	</target>

</project>
